{
  "manifest_version": 3,
  "name": "Hid Homepage",
  "version": "0.2.0-alpha",
  "description": "It is your customizable minimalist homepage for your browser.",
  "icons": {
    "48": "./public/vite.svg"
  },
  "permissions": ["bookmarks", "storage", "tabs"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Hid Homepage"
  }
}
